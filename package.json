{
  "name": "monorepo-test",
  "version": "1.0.0",
  "description": "Monorepo with React frontend and Express backend",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julioacostapallud/monorepoTest.git"
  },
  "keywords": [
    "monorepo",
    "react",
    "express",
    "vite",
    "netlify",
    "vercel"
  ],
  "author": "Julio Cesar Acosta",
  "license": "MIT"
}
